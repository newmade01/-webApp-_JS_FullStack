option>\r\n      <option value=\"7\">파트교체</option>\r\n      <option value=\"8\">MIM</option>\r\n      <option value=\"9\">Xray</option>\r\n    </select>\r\n  </li>\r\n  <li>\r\n  <h3>소분류</h3>\r\n    <select class=\"s1\">\r\n      <option >소분류 선택</option>\r\n    </select>\r\n  </li>\r\n\r\n  </ul>\r\n\r\n\r\n\r\n <br />\r\n\r\n  <router-link :to=\"{ name: 'canvas' }\" style=\"background:yellow font-size: medium\">서명하기</router-link>   \r\n  <v-btn @click=\"textUpload\">서비스 리포트 입력내용 전송</v-btn>\r\n\r\n  <v-container fluid fill-height>\r\n    <v-layout align-center justify-center>\r\n      <v-flex xs12 sm8 md4>\r\n        <v-card class=\"elevation-12\">\r\n          <v-card-title>서비스 리포트 이미지 보내기</v-card-title>\r\n          <v-card-text>\r\n            <input id=\"bin\" type=\"file\">\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-btn @click=\"upload\">이미지 전송</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-flex>\r\n    </v-layout>\r\n  </v-container>\r\n\r\n\r\n  \r\n  </div>\r\n</div>\r\n</template>\r\n<script src=\"https://code.jquery.com/jquery-3.5.1.min.js\"></script>\r\n<script>\r\n// 대분류 소분류 선택\r\nimport $ from \"jquery\";\r\n$(document).ready(function(){\r\n  var install=['SIGMA || 설치','110HQ 설치','210 설치', 'DR 일반촬영실 설치','DR 포터블 설치', 'DR로밍시스템 설치', '기존장비 이전 설치','중고장비 설치', 'US 설치'];\r\n  var AS=['CR장비 긴급 고장', 'DR장비 긴급 고장','PACS 관련 긴급 고장','납품 콘솔 긴급 고장', 'X-Ray 관련 긴급 고장', 'US장비 긴급 고장', 'Sigma롤러 청소','콘솔 재설치'];\r\n  var PM=['정기 예방점검 진행', '이미지 예방점검 진행', 'X-ray 예방점검 진행','이미지 보정 및 점검','패널보정 및 점검','코드맵핑 및 점검','패널보정 및 이미지 보정']\r\n  var support=['오픈지원','사용자 교육', '맘모 정도관리 지원', '데모지원', '설치 후 콘솔셋팅', '이미지보정 방문요청']\r\n  var remote=['(원격)PACS 뷰어설치','(원격) 모달리티 연동지원', '(원격)콘솔서비스 지원','(원격)서버장애처리','원격판독','(원격)PACS 사용자 교육', '(원격)CS7 사용자 교육','(원격) OCS 연동', '(원격)PACS 장애처리', '(원격)이미지 보정', '(원격)PPP 서비스', '(원격)게이트웨이 서비스', '(원격)콘솔 장애처리', '(원격)이미지파일럿 장애처리']\r\n  var US=['임상지원','학회지원']\r\n  var part=['Halogen Lamp 교체', 'FAN 교체', 'GIUDE 교체', 'Tumbler 교체', 'MDU보드 교체', 'LPB, LLB 교체','Optical Unit 교체', 'Sigma Eraser Unit 교체', 'Sigma MainBoard 교체', 'MCB 보드 교체', 'CST BackPlate 교체', 'SW Power Board 교체', '필터교체(드라이프로)','필터교체(CR)']\r\n  var MIM=['MIM 설치', 'MIM -PW', 'MIM A/S','MIM DEMO','MIM 학회','MIM 교육',]\r\n  var Xray=['방향키 AS', '콜리메이터 AS', '핸드스위치 AS', '콘솔멤브레인 AS', '제너레이터 AS', 'Tube AS']\r\n\r\n  $('.s0').change(function(){\r\n    var sel= $(this).val();\r\n    if(sel==1){\r\n      $('.op').remove();\r\n      $.each(install, function(i, item){\r\n        $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n      });\r\n    }\r\n    else if(sel==2){\r\n       $('.op').remove();\r\n        $.each(AS, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==3){\r\n       $('.op').remove();\r\n        $.each(PM, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==4){\r\n       $('.op').remove();\r\n        $.each(support, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==5){\r\n       $('.op').remove();\r\n        $.each(remote, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==6){\r\n       $('.op').remove();\r\n        $.each(US, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==7){\r\n       $('.op').remove();\r\n        $.each(part, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n    else if(sel==8){\r\n       $('.op').remove();\r\n        $.each(MIM, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n   \r\n    else{\r\n       $('.op').remove();\r\n        $.each(Xray, function(i, item){\r\n             $('.s1').append('<option class=\"op\">'+item+'</option>');\r\n           \r\n        });\r\n    }\r\n  })\r\n})\r\nrequire('date-utils');\r\nimport Vue from \"vue\";\r\nimport axios from 'axios';\r\nimport imageCompressor from 'vue-image-compressor'\r\n\r\n const multer = require('multer');\r\nexport default {\r\n\r\n  \r\n\r\n\r\n  components: { imageCompressor },\r\n\r\n  data: () => ({\r\n    selectOne: '', //대분류 값\r\n    files: [],\r\n    date: \"\",\r\n    name: \"\",\r\n    contents: \"\",\r\n     form: {\r\n      name: '서비스 리포트 이미지 업로드'\r\n    },\r\n     img: \"\",\r\n    scale: 100,\r\n    quality: 50,\r\n    originalSize: true,\r\n    original: {},\r\n    searchTerm: '',\r\n\r\n  }),\r\n\r\n  methods: {\r\n\r\n   \r\n   \r\n    \r\n\r\n    //이미지 저장  \r\n    upload () {\r\n      const fd = new FormData()\r\n      fd.append('name', this.form.name)\r\n      fd.append('bin', document.getElementById('bin').files[0])\r\n\r\n      axios.post('http://192.168.1.149:3002/ind/image', fd)\r\n        .then((req, res) => {\r\n         alert('이미지 저장 성공')\r\n        //  console.log( document.getElementById('bin').files[0])\r\n      \r\n        })\r\n        .catch(e => {\r\n         alert('실패')\r\n        })\r\n    },\r\n\r\n\r\n    //텍스트 INSERT넣기 서비스 리포트\r\n    textUpload:function() {  \r\n          //파일명 guidname 추가\r\n          var newDate=new Date();\r\n          var time=newDate.toFormat('YYYYMMDD');        \r\n          var date=time;\r\n         console.log(date)\r\n\r\n          var params = new URLSearchParams();\r\n          params.append('date', date);\r\n          params.append('searchText', this.searchText);\r\n          params.append('searchName', this.searchName);\r\n          params.append('searchRegdate', this.searchRegdate);\r\n          // params.append('selectOne', this.selectOne);\r\n\r\n             \r\n          axios\r\n            .post('http://192.168.1.149:3002/ind/serviceReportSend', params)\r\n            .then(res => {\r\n            console.log(res.data)\r\n              // //첫번째 배열 가져오기\r\n              // const InfoArray = Object.values(res.data[0])\r\n              this.items=res.data      \r\n              console.log(res)   \r\n              console.log('성공')\r\n              alert('내용 보내기 성공')\r\n            })\r\n            .catch(function (err) {\r\n              console.log(err);\r\n            })\r\n    },\r\n\r\n\r\n    \r\n  getFiles(obj){\r\n      this.img = obj.compressed.blob\r\n      this.original = obj.original\r\n      this.compressed = obj.compressed\r\n  },\r\n  \r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#top {\r\n  width: 294px;\r\n  height: 40.2px;\r\n}\r\n\r\n#text1 {\r\n  width: 147px;\r\n  height: 21px;\r\n  color: rgba(0, 0, 0, 0.74);\r\n}\r\n#box {\r\n  height: 30px;\r\n  background-color: #99d0e2;\r\n}\r\n\r\n#name {\r\n  width: 414px;\r\n  height: 896px;\r\n  color: #ffffff;\r\n}\r\n\r\n#searchBTN {\r\n  width: 47px;\r\n  height: 34px;\r\n  margin: 26px 38px 60px 0;\r\n  border-radius: 11px;\r\n}\r\n\r\n#addBTN {\r\n  width: 47px;\r\n  height: 34px;\r\n  margin: 26px 38px 60px 0;\r\n  border-radius: 11px;\r\n}\r\n\r\n#searchBox {\r\n  border-radius: 9px;\r\n  border: solid 1px #707070;\r\n  background-color: #ffffff;\r\n  margin: 30px 11px 60px 38px;\r\n  padding: 7px 115px 6px 112px;\r\n  height: 30px;\r\n  width: 280px;\r\n}\r\n#placeHolder {\r\n  width: 53px;\r\n  height: 17px;\r\n  font-size: 15px;\r\n  font-weight: normal;\r\n  font-stretch: normal;\r\n  line-height: 1.13;\r\n  letter-spacing: normal;\r\n  text-align: left;\r\n  color: rgba(77, 58, 58, 0.75);\r\n}\r\n\r\n#text {\r\n  font-size: 15px;\r\n\r\n  line-height: 1.13;\r\n\r\n  color: #9d9d9d;\r\n}\r\n\r\n#button {\r\n  width: 257px;\r\n  height: 27px;\r\n  margin: 28px 21px 29px 0;\r\n  border-radius: 5px;\r\n  border: solid 1px #707070;\r\n  background-color: #a0cc00;\r\n}\r\n\r\n#AddButton {\r\n  color: #ffffff;\r\n}\r\n\r\n#selectButton {\r\n  width: 153px;\r\n  height: 22px;\r\n  margin: 7px 92px 39px 14px;\r\n  border-radius: 10px;\r\n  border: solid 1px #707070;\r\n  background-color: #ffffff;\r\n}\r\n\r\n#input {\r\n  width: 228px;\r\n  height: 100;\r\n  margin: 38px 44px 0 14px;\r\n  border: solid 1px #423c3c;\r\n}\r\n#textarea {\r\n  width: 228px;\r\n  border: solid 1px #423c3c;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}